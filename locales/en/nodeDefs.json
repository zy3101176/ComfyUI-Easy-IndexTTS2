{
  "easy downloadIndexTTSAndLoadModel": {
    "display_name": "IndexTTS Model Loader",
    "inputs": {
      "model": {
        "name": "Model"
      },
      "download_from": {
        "name": "Download From"
      },
      "start": {
        "name": "Start",
        "tooltip": "Start to load or download models"
      }
    },
    "outputs": {
      "0": {
        "name": "IndexTTS Model"
      }
    }
  },
  "easy indexTTSGenerate": {
    "display_name": "IndexTTS Generate",
    "inputs": {
      "indextts_model": {
        "name": "IndexTTS Model"
      },
      "reference_audio": {
        "name": "Reference Audio",
        "tooltip": "Reference audio for voice cloning (Only the one voice is supported)"
      },
      "reference_audios": {
        "name": "Reference Audios",
        "tooltip": "(Optional) Reference audios for voice cloning (Multiple voices are supported)"
      },
      "emotions": {
        "name": "Emotions",
        "tooltip": "(Optional) voice emotions"
      },
      "text": {
        "name": "Text",
        "tooltip": "Text to synthesize. Supports pause format: -2s- (2 seconds pause), -0.5s- (0.5 seconds pause)"
      },
      "unload_model": {
        "name": "Unload Model",
        "tooltip": "Unload model from VRAM after synthesis"
      },
      "do_sample": {
        "name": "Do Sample"
      },
      "temperature": {
        "name": "Temperature"
      },
      "top_p": {
        "name": "Top P"
      },
      "top_k": {
        "name": "Top K"
      },
      "num_beams": {
        "name": "Num Beams"
      },
      "repetition_penalty": {
        "name": "Repetition Penalty"
      },
      "length_penalty": {
        "name": "Length Penalty"
      },
      "max_mel_tokens": {
        "name": "Max Mel Tokens"
      },
      "max_tokens_per_sentence": {
        "name": "Max Tokens Per Sentence"
      },
      "speech_speed": {
        "name": "Speech Speed",
        "tooltip": "Speech speed (0.8=slower, 1.0=normal, 1.3=faster)"
      },
      "seed": {
        "name": "Seed"
      }
    },
    "outputs": {
      "0": {
        "name": "Audio"
      },
      "1": {
        "name": "Seed"
      },
      "2": {
        "name": "Subtitle"
      }
    }
  },
